digraph types {
    rankdir=LR;
    node [style=filled];

    "Circle" [shape=box, fillcolor="#ADD8E6"];
    "Color" [shape=box, fillcolor="#90EE90"];
    "LineCap" [shape=box, fillcolor="#90EE90"];
    "LineJoin" [shape=box, fillcolor="#90EE90"];
    "Styled" [shape=box, fillcolor="#ADD8E6"];
    "float32" [shape=ellipse, fillcolor="#FFFFE0"];

    "Circle" -> "float32" [label="cx"];
    "Circle" -> "float32" [label="cy"];
    "Circle" -> "float32" [label="r"];
    "Circle" -> "Styled" [label="(implements)"];
    "Styled" -> "Color" [label="fill"];
    "Styled" -> "float32" [label="fill_opacity"];
    "Styled" -> "Color" [label="stroke"];
    "Styled" -> "float32" [label="stroke_width"];
    "Styled" -> "LineCap" [label="stroke_linecap"];
    "Styled" -> "LineJoin" [label="stroke_linejoin"];
    "Styled" -> "float32" [label="opacity"];
}
