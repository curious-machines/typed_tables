digraph types {
    rankdir=LR;
    node [style=filled];
    label="graph Boss stored origin";
    labelloc=t;
    fontsize=18;

    "Boss" [shape=box, fillcolor="#ADD8E6", penwidth=3];
    "Faction" [shape=box, fillcolor="#90EE90"];
    "boolean" [shape=ellipse, fillcolor="#FFFFE0"];
    "float32" [shape=ellipse, fillcolor="#FFFFE0"];
    "int16" [shape=ellipse, fillcolor="#FFFFE0"];
    "string" [shape=ellipse, fillcolor="#FFFFE0"];
    "uint16" [shape=ellipse, fillcolor="#FFFFE0"];
    "uint32" [shape=ellipse, fillcolor="#FFFFE0"];
    "uint64" [shape=ellipse, fillcolor="#FFFFE0"];
    "uint8" [shape=ellipse, fillcolor="#FFFFE0"];

    "Boss" -> "string" [label="name\n(from Identifiable)"];
    "Boss" -> "float32" [label="x\n(from Positioned)"];
    "Boss" -> "float32" [label="y\n(from Positioned)"];
    "Boss" -> "uint32" [label="id\n(from Entity)"];
    "Boss" -> "uint64" [label="created_at\n(from Timed)"];
    "Boss" -> "int16" [label="hp\n(from Damageable)"];
    "Boss" -> "int16" [label="max_hp\n(from Damageable)"];
    "Boss" -> "uint16" [label="attack\n(from Combatant)"];
    "Boss" -> "uint16" [label="defense\n(from Combatant)"];
    "Boss" -> "string" [label="sprite\n(from Renderable)"];
    "Boss" -> "uint8" [label="layer\n(from Renderable)"];
    "Boss" -> "float32" [label="width\n(from Physical)"];
    "Boss" -> "float32" [label="height\n(from Physical)"];
    "Boss" -> "boolean" [label="solid\n(from Physical)"];
    "Boss" -> "float32" [label="speed\n(from Creature)"];
    "Boss" -> "uint8" [label="level\n(from Creature)"];
    "Boss" -> "Faction" [label="faction\n(from Creature)"];
    "Boss" -> "string" [label="greeting\n(from NPC)"];
    "Boss" -> "boolean" [label="quest_giver\n(from NPC)"];
    "Boss" -> "uint8" [label="phase"];
    "Boss" -> "string" [label="title"];
    "Boss" -> "uint32" [label="reward_gold"];
}
