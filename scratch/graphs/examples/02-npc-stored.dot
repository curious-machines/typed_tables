digraph types {
    rankdir=LR;
    node [style=filled];
    label="graph NPC stored";
    labelloc=t;
    fontsize=18;

    "Faction" [shape=box, fillcolor="#90EE90"];
    "NPC" [shape=box, fillcolor="#ADD8E6", penwidth=3];
    "boolean" [shape=ellipse, fillcolor="#FFFFE0"];
    "float32" [shape=ellipse, fillcolor="#FFFFE0"];
    "int16" [shape=ellipse, fillcolor="#FFFFE0"];
    "string" [shape=ellipse, fillcolor="#FFFFE0"];
    "uint16" [shape=ellipse, fillcolor="#FFFFE0"];
    "uint32" [shape=ellipse, fillcolor="#FFFFE0"];
    "uint64" [shape=ellipse, fillcolor="#FFFFE0"];
    "uint8" [shape=ellipse, fillcolor="#FFFFE0"];

    "NPC" -> "string" [label="name"];
    "NPC" -> "float32" [label="x"];
    "NPC" -> "float32" [label="y"];
    "NPC" -> "uint32" [label="id"];
    "NPC" -> "uint64" [label="created_at"];
    "NPC" -> "int16" [label="hp"];
    "NPC" -> "int16" [label="max_hp"];
    "NPC" -> "uint16" [label="attack"];
    "NPC" -> "uint16" [label="defense"];
    "NPC" -> "string" [label="sprite"];
    "NPC" -> "uint8" [label="layer"];
    "NPC" -> "float32" [label="width"];
    "NPC" -> "float32" [label="height"];
    "NPC" -> "boolean" [label="solid"];
    "NPC" -> "float32" [label="speed"];
    "NPC" -> "uint8" [label="level"];
    "NPC" -> "Faction" [label="faction"];
    "NPC" -> "string" [label="greeting"];
    "NPC" -> "boolean" [label="quest_giver"];
}
