digraph types {
    rankdir=LR;
    node [style=filled];
    label="graph MagicWeapon path to Identifiable";
    labelloc=t;
    fontsize=18;

    "Entity" [shape=box, fillcolor="#FFB347"];
    "GameEntity" [shape=box, fillcolor="#FFB347"];
    "Identifiable" [shape=box, fillcolor="#FFB347"];
    "Item" [shape=box, fillcolor="#ADD8E6"];
    "MagicWeapon" [shape=box, fillcolor="#ADD8E6", penwidth=3];
    "Weapon" [shape=box, fillcolor="#ADD8E6"];
    "WorldObject" [shape=box, fillcolor="#FFB347"];

    "MagicWeapon" -> "Weapon" [label="(extends)", style=dashed];
    "Weapon" -> "Item" [label="(extends)", style=dashed];
    "Item" -> "WorldObject" [label="(implements)", style=dashed];
    "WorldObject" -> "GameEntity" [label="(extends)", style=dashed];
    "GameEntity" -> "Entity" [label="(extends)", style=dashed];
    "Entity" -> "Identifiable" [label="(extends)", style=dashed];
}
