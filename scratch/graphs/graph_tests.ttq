use "graph_tests"

alias coordinate = float32
alias length = float32
alias uuid = uint128

interface Nameable {
    name: string
}

interface Positionable {
    x: coordinate,
    y: coordinate
}

interface Sizeable {
    width: length,
    height: length
}

interface Address {
    street: string,
    city: string,
    state: string,
    zip: string
}

type Base from Nameable, Positionable, Sizeable {
    age: uint8
}

type Root from Base {
    id: uuid,
    address: Address
}

type Person from Nameable {
    id: uuid,
    age: uint8
}
