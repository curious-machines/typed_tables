-- Interfaces

interface Nameable {
    name: string
}

interface Positionable {
    x: coordinate,
    y: coordinate
}

interface Sizeable {
    width: length,
    height: length
}

interface Address {
    street: string,
    city: string,
    state: State,
    zip: string
}

-- Composites

type Base from Nameable, Positionable, Sizeable {
    age: uint8
}

type Root from Base {
    id: uuid,
    address: Address,
    label_color: Color,
    data: {string: string}
}

type Person from Nameable {
    id: uuid,
    age: uint8,
    nicknames: {string},
    code: [uint8]
}
